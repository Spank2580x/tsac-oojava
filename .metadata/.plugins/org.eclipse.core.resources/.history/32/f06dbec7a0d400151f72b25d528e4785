package main;

/**
* NON MODIFICARE
*/
public interface PressureSensorListener {

	/**
	 * Metodo chiamato in caso di lettura corretta.
	 * @param pressure
	 */
	double onSuccess(double pressure);
	
	/**
	 * Metodo chiamato in caso di errore nella lettura.
	 * @param exception
	 */
	PressureSensorException onFailed(PressureSensorException exception);
	
}
